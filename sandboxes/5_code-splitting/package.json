{
  "name": "@webpack-how/5_code-splitting",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "scripts": {
    "start-ep": "webpack serve --open --config entry-points_webpack.config.js",
    "build-ep": "webpack --config entry-points_webpack.config.js",
    "start-sc": "webpack serve --open --config split-chunks_webpack.config.js",
    "build-sc": "webpack --config split-chunks_webpack.config.js",
    "start-di": "webpack serve --open --config dynamic-imports_webpack.config.js",
    "build-di": "webpack --config dynamic-imports_webpack.config.js",
    "analyse": "webpack --config split-chunks_webpack.config.js --profile --json > stats.json"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.3",
    "lodash": "^4.17.21",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
